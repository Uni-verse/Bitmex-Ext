{
    "name": "Bitmex Extension",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Bitmex Extension",
    "content_scripts": [{
        "matches": ["http://*/*/*", "https://*/*/*"],
        "run_at": "document_end",
        "js": ["jquery.js", "main.js"]
    }],
    "permissions": [
		"<all_urls>",
		"tabs",
		"storage",
		"clipboardRead"
	]
}